{
  "name": "hcaptcha_flutter",
  "version": "0.0.1",
  "summary": "HCaptcha for Flutter.",
  "description": "HCaptcha for Flutter.",
  "homepage": "https://kjxbyz.com",
  "license": {
    "file": "../LICENSE"
  },
  "authors": {
    "kjxbyz": "kjxbyz@163.com"
  },
  "source": {
    "path": "."
  },
  "source_files": "Classes/**/*.{h,m,swift}",
  "public_header_files": "Classes/**/*.h",
  "module_map": "Classes/HCaptchaFlutterPlugin.modulemap",
  "static_framework": true,
  "dependencies": {
    "Flutter": [],
    "HCaptcha": [
      "~> 2.7.0"
    ]
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": "5.0",
  "xcconfig": {
    "LIBRARY_SEARCH_PATHS": "$(TOOLCHAIN_DIR)/usr/lib/swift/$(PLATFORM_NAME)/ $(SDKROOT)/usr/lib/swift",
    "LD_RUNPATH_SEARCH_PATHS": "/usr/lib/swift"
  },
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386"
  },
  "swift_version": "5.0"
}
